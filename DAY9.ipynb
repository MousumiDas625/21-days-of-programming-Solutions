    "#defining the function\n",
    "def sumofoddfactors(n):\n",
    "    sum=0\n",
    "    for i in range(1,n+1):\n",
    "        if(n%i==0 and (i%2)!=0):\n",
    "            sum=sum+i\n",
    "    return sum \n",
    "\n",
    "#Taking input\n",
    "n=int(input(\"Enter the number \"))\n",
    "\n",
    "#calling the function\n",
    "result=sumofoddfactors(n)\n",
    "print(result)"
